
<table class = "table table-sm table-bordered table-striped">
    <thead>
        <tr><th>Key</th><th>Name</th><th>In Active Use</th><th>Foreign SSN</th><th>Foreign First Name</th><th>Foreign Family Name</th></tr>
    </thead>
    <tbody>
       
            <tr class = "placeholder"><td colspan = "6" class = "text-center">No Data</td></tr>
        
         @foreach (SecondaryIdentity s in ViewBag.Secondaries)
           {
               <tr>
                   <td>@s.Id</td>
                   <td>@s.Name</td>
                   <td>@s.InActiveUse.ToString()</td>
                    <td>@(s.PrimarySSN ?? "(null)")</td>
                   <td>@(s.PrimaryFirstName ?? "(null)")</td>
                   <td>@(s.PrimaryFamilyName ?? "(null)")</td>
                  
               </tr>
           }
       
    </tbody>
</table>